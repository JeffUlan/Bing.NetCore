<Project>
  <PropertyGroup>
    <AssemblyName>Bing.EasyCaching</AssemblyName>
    <PackageId>Bing.EasyCaching</PackageId>
    <Description>Bing.EasyCaching是实现缓存操作类库。</Description>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Bing\Bing.csproj" />
  </ItemGroup>

  <ItemGroup Condition=" '$(TargetFramework)' == 'netstandard2.0' ">
    <PackageReference Include="EasyCaching.Core" Version="0.6.1" />
  </ItemGroup>

  <ItemGroup Condition=" '$(TargetFramework)' == 'netcoreapp3.1' ">
    <PackageReference Include="EasyCaching.Core" Version="1.1.0" />
  </ItemGroup>

</Project>